    "@types/material-ui": "^0.17.2",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.13",
    "@types/react": "15.0.8",
    "@types/react-dom": "0.14.23",
    "@types/react-redux": "^4.4.39",
    "@types/react-router": "3.0.4",
    "@types/react-router-redux": "4.0.41",
    "@types/react-tap-event-plugin": "^0.0.30",
    "@types/redux-saga": "^0.10.5",
    "@types/should": "^8.3.0",
    "@types/sinon": "^2.1.3",
    "awesome-typescript-loader": "3.0.3",
    "babel-core": "^6.24.1",
    "cross-env": "^4.0.0",
